	pageHref = window.location.href;

	var getPageUrl = function() {
		// When used with rewrite rules, pathname will be '/' for home page
		if (window.location.pathname === "/") {
			return window.location.href;
		} else {
			return window.location.pathname;
		}
	}
	var deleteCookie = function(cookieList) {
		if (cookieList != undefined && cookieList.length > 0)
			cookieList.forEach(function(key) {
				document.cookie = key + "=; expires=Thu, 01 Jan 1970 00:00:01 GMT" + "; path=/";
			});
	}

	if (Wegmans.siteRootPath.length === 0 && Wegmans.isNonCachedPage && getCookie("wegmans_refresh").length === 0) {
		document.title = "Sign In - Wegmans"; // Update page title while it refreshes
		document.cookie = "wegmansRedirect=" + window.location.href + " " + ";path=/";
		window.location.href = Wegmans.siteRootPath + "/sign-in.html";
	}


	if (getCookie("wegmans_refresh").length !== 0 && getCookie("wegmans_access").length === 0) {
		var path = getPageUrl() + "/j_security_check";
		var xhr = new XMLHttpRequest();
		xhr.open("GET", path, false);
		xhr.onload = function(e) {
			if (xhr.readyState === 4) {
				if (xhr.status === 200) {} else {}
			}
		};
		xhr.onerror = function(e) {};
		xhr.send(null);
	} else if (getCookie("wegmans_refresh").length === 0) {
		var cookieList = ["wegmans_user", "wegmans_user_encoded", "wegmans_access"];
		deleteCookie(cookieList);
	}

	// Reset password for logged in user
	if (pageHref.indexOf("/change-password.html?key=") != -1 && getCookie("wegmans_refresh").length !== 0 && getCookie("wegmans_access").length !== 0 && pageHref.length - (pageHref.indexOf("=") + 1) > 0) {
		var cookieList = ["wegmans_user", "wegmans_user_encoded", "wegmans_access", "wegmansRedirect", "wegmans_refresh"];
		deleteCookie(cookieList);
		location.reload();
	}

