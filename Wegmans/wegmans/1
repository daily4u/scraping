    <script type="text/javascript">
        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        function getPreferredStored() {
            var wegmans_cookies = getCookie('wegmans_user_encoded');
            if (wegmans_cookies !== "") {
                var cookie_json = JSON.parse(window.atob(wegmans_cookies));
                var store_id = 'LOCATION_' + cookie_json.wfm_profile_store;
                return store_id;

            } else {
                var custom_cookie = getCookie("wegmans-custom");
                if (custom_cookie !== "") {
                    var custom_cookie_value = JSON.parse(custom_cookie);
                    return custom_cookie_value.storeInfo.storeId;
                    digitalData.registration = {
                        accountCreatedStoreId: custom_cookie_value.storeInfo.storeId
                    };
                }
            }
        }

        var wegmans_cookies = getCookie('wegmans_user_encoded');
        var status = "Anonymous";
        var shoppersClub = "";
        var customerId = "";
        if (wegmans_cookies !== "") {
            status = "Logged-In";
            var cookie_json = JSON.parse(window.atob(wegmans_cookies));
            shoppersClub = cookie_json.wfm_shoppersClubNumber;
            customerId = cookie_json.wfm_profile_cuid;
        }

        var digitalData = digitalData || {};
        digitalData.page = {
            pageInfo: {
                pageName: "Home:Products:Baby Essentials",
                loggedInStatus: status
            },
            category: {
                pageType: "deep department",
                primaryCategory: "Products",
                subCategory2: "",
                subCategory3: "",
                subCategory4: "",
                siteName: "wegmans.com",
                language: "English",
                baseShopperNumber: shoppersClub
            },
            event: {
                storeDetailView: false
            }
        };

        digitalData.registration = {
            accountCreatedStoreId: getPreferredStored()
        };

    </script>


